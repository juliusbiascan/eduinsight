{
  "extends": "tslint:recommended",
  "defaultSeverity": "error",
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-shadowed-variable": false,
    "variable-name": [true, "allow-leading-underscore"],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import [^,]+ from |^export | implements"
      }
    ]
  },
  "jsRules": {}
}
